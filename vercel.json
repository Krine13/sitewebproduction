{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/:path",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    {
      "source": "/createursiteweb",
      "destination": "/createur-site-web",
      "permanent": true
    },
    {
      "source": "https://sitewebproduction.com/:path*",
      "destination": "https://www.sitewebproduction.com/:path*",
      "permanent": true
    }
  ]
}




  